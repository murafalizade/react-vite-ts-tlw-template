{
  "name": "cra-template-react-vite-ts-tlw-template",
  "version": "0.0.3",
  "type": "module",
  "license": "MIT",
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/murafalizade/react-vite-ts-tlw-template.git"
  },
  "bugs": {
    "url": "https://github.com/murafalizade/react-vite-ts-tlw-template/issues"
  },
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "redux",
    "reduxjs",
    "redux-toolkit",
    "react-redux",
    "i18next"
  ],
  "scripts": {
    "build:css": "postcss src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "dev": "template/node_modules/.bin/vite",
    "build": "template/node_modules/.bin/tsc && vite build && NODE_ENV=production postcss src/styles/tailwind.css -o dist/styles/tailwind.css",
    "lint": "template/node_modules/.bin/eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "template/node_modules/.bin/vite preview"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.32",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "i18next": "^23.7.7",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-redux": "^9.0.2",
    "react-router-dom": "^6.20.1",
    "redux": "^5.0.0",
    "sweetalert2": "^11.10.1",
    "tailwindcss": "^3.3.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
